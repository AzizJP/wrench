{
  "compilerOptions": {
    "target": "es6",
    "outDir": "./dist/",
    "sourceMap": true,
    "noEmit": false,
    "noImplicitAny": true,
    "allowJs": true,
    "module": "esnext",
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}